{
    "manifest_version": 2.0,
    "id": "zabbix-cmdb",
    "name": "Zabbix CMDB",
    "author": "火星小刘",
    "url": "https://github.com/X-Mars/zabbix_modules",
    "version": "1.2.0",
    "namespace": "ZabbixCmdb",
    "description": "Configuration Management Database for Zabbix - Optimized with pagination support",
    "actions": {
        "cmdb": {
            "class": "Cmdb",
            "view": "cmdb"
        },
        "cmdb.groups": {
            "class": "CmdbGroups",
            "view": "cmdb.groups"
        }
    }
}
